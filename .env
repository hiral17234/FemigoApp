# These keys are for Firebase and must be provided for the app to work.
# Find them in your Firebase project settings: Project Settings > General
NEXT_PUBLIC_FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY_HERE
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=YOUR_FIREBASE_AUTH_DOMAIN_HERE
NEXT_PUBLIC_FIREBASE_PROJECT_ID=YOUR_FIREBASE_PROJECT_ID_HERE
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=YOUR_FIREBASE_STORAGE_BUCKET_HERE
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=YOUR_FIREBASE_MESSAGING_SENDER_ID_HERE
NEXT_PUBLIC_FIREBASE_APP_ID=YOUR_FIREBASE_APP_ID_HERE

# This key is for Google Maps and MUST start with "AIza"
# Find it in your Google Cloud Console under "APIs & Services" > "Credentials"
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# This key is for Google AI (Gemini) and MUST start with "AIza"
# Generate it from Google AI Studio: https://aistudio.google.com/app/apikey
NEXT_PUBLIC_GOOGLE_AI_KEY=AIzaSyCiNoDkjmlhg8Oh59x9VEEseiSfXtCPPmc

# This key is for the Hugging Face model and is used ONLY on the server.
# Find it on Hugging Face: Settings > Access Tokens > New token (read role)
# DO NOT add NEXT_PUBLIC_ to this key name.
HUGGINGFACE_API_KEY=YOUR_HUGGINGFACE_API_KEY_HERE
GEMINI_API_KEY=